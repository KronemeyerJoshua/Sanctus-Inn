{
    "private": true,
    "scripts": {
        "dev": "npm run development && npm run development-css",
        "development": "cross-env COMP_TYPE=js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "development-css": "cross-env COMP_TYPE=css NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-css": "npm run development-css -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production && npm run production-css",
        "production": "cross-env COMP_TYPE=js NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production-css": "cross-env COMP_TYPE=css NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.19.2",
        "cross-env": "^5.1",
        "laravel-mix": "^5.0.4",
        "lodash": "^4.17.13",
        "resolve-url-loader": "^2.3.1",
        "laravel-mix-bundle-analyzer": "^1.0.5",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.43.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/vue-fontawesome": "^0.1.9",
        "any-base": "^1.1.0",
        "bulma": "^0.8.2",
        "bulma-toast": "^1.5.10",
        "dayjs": "^1.8.27",
        "tippy.js": "^6.2.3",
        "vue-router": "^3.2.0",
        "vuelidate": "^0.7.5",
        "vuex": "^3.4.0"
    }
}
